{
    "name": "edisoncor-light-theme",
    "type": "light",
    "colors": {
        "activityBarBadge.background": "#005f87",
        "editor.background": "#FFFFFF",
        "editor.foreground": "#000000",
        "editor.inactiveSelectionBackground": "#D3D3D3",
        "editor.lineHighlightBackground": "#E0E0E0",
        "editor.selectionBackground": "#A8A8A8",
        "editorCursor.foreground": "#FF0000",
        "editorIndentGuide.activeBackground": "#A0A0A0",
        "editorIndentGuide.background": "#D0D0D0",
        "editorLineNumber.foreground": "#606060",
        "gitDecoration.addedResourceForeground": "#008000",
        "gitDecoration.conflictingResourceForeground": "#FFA500",
        "gitDecoration.modifiedResourceForeground": "#005f87",
        "list.activeSelectionForeground": "#FFD700",
        "sideBar.background": "#F0F0F0",
        "statusBar.background": "#005f87",
        "statusBar.foreground": "#FFFFFF",
        "statusBarItem.activeBackground": "#005f87",
        "statusBarItem.hoverBackground": "#005f87"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#808080",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#008000"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#0000FF"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#B8860B"
            }
        },
        {
            "scope": "constant",
            "settings": {
                "foreground": "#B22222"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#B8860B"
            }
        },
        {
            "scope": "type",
            "settings": {
                "foreground": "#4682B4"
            }
        },
        {
            "scope": "class",
            "settings": {
                "foreground": "#B8860B"
            }
        },
        {
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "#8A2BE2"
            }
        },
        {
            "scope": "string.quoted.double.json",
            "settings": {
                "foreground": "#32CD32"
            }
        },
        {
            "scope": "punctuation.separator.key-value.json",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": "punctuation.definition.string.begin.json",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": "punctuation.definition.string.end.json",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": "constant.language.boolean",
            "settings": {
                "foreground": "#0000FF"
            }
        }
    ]
}