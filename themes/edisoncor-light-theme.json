{
    "name": "edisoncor-theme",
    "type": "light",
    "colors": {
        "activityBarBadge.background": "#007acc",
        "editor.background": "#FFFFFF",
        "editor.foreground": "#000000",
        "editor.inactiveSelectionBackground": "#D3D3D3",
        "editor.lineHighlightBackground": "#F0F0F0",
        "editor.selectionBackground": "#ADD8E6",
        "editorCursor.foreground": "#000000",
        "editorIndentGuide.activeBackground": "#A0A0A0",
        "editorIndentGuide.background": "#D3D3D3",
        "editorLineNumber.foreground": "#2B91AF",
        "gitDecoration.addedResourceForeground": "#008000",
        "gitDecoration.conflictingResourceForeground": "#FF0000",
        "gitDecoration.modifiedResourceForeground": "#0000FF",
        "list.activeSelectionForeground": "#0000FF",
        "sideBar.background": "#F3F3F3",
        "statusBar.background": "#007acc",
        "statusBar.foreground": "#FFFFFF",
        "statusBarItem.activeBackground": "#007acc",
        "statusBarItem.hoverBackground": "#005f9e"
    },
    "tokenColors": [
        {
            "scope": [
                "keyword.control",
                "storage.type",
                "entity.name.tag"
            ],
            "settings": { "foreground": "#0000FF", "fontStyle": "bold" }
        },
        {
            "scope": "entity.name.type.class",
            "settings": { "foreground": "#FF8C00", "fontStyle": "bold" }
        },
        {
            "scope": [
                "entity.name.function",
                "support.type.property-name.css"
            ],
            "settings": { "foreground": "#008000" }
        },
        {
            "scope": [
                "variable.parameter",
                "entity.name.keyword.java",
                "support.type.property-name.json",
                "storage.modifier"
            ],
            "settings": { "foreground": "#800080", "fontStyle": "bold" }
        },
        {
            "scope": "constant",
            "settings": { "foreground": "#FF4500" }
        },
        {
            "scope": "string.quoted",
            "settings": { "foreground": "#DAA520" }
        },
        {
            "scope": "constant.numeric",
            "settings": { "foreground": "#8A2BE2" }
        },
        {
            "scope": "comment",
            "settings": { "foreground": "#808080", "fontStyle": "bold italic" }
        },
        {
            "scope": "keyword.operator",
            "settings": { "foreground": "#FF6347" }
        },
        {
            "scope": [
                "meta.decorator",
                "keyword.control.import.java",
                "keyword.control.package.java"
            ],
            "settings": { "foreground": "#4682B4", "fontStyle": "italic" }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": { "foreground": "#FF8C00" }
        },
        {
            "scope": "string.quoted.double.html, string.quoted.single.html",
            "settings": { "foreground": "#008000" }
        },
        {
            "scope": "comment.block.html, comment.block.xml",
            "settings": { "foreground": "#808080", "fontStyle": "bold italic" }
        }
    ]
}