{
    "name": "edisoncor-theme",
    "type": "dark",
    "colors": {
        "activityBarBadge.background": "#1593a2",
        "editor.background": "#1D272C",
        "editor.foreground": "#f2ebe2",
        "editor.inactiveSelectionBackground": "#3A3D41",
        "editor.lineHighlightBackground": "#3E3E3E",
        "editor.selectionBackground": "#515151",
        "editorCursor.foreground": "#FFCC00",
        "editorIndentGuide.activeBackground": "#707070",
        "editorIndentGuide.background": "#3E3E3E",
        "editorLineNumber.foreground": "#4B4B4B",
        "gitDecoration.addedResourceForeground": "#459923",
        "gitDecoration.conflictingResourceForeground": "#FFA500",
        "gitDecoration.modifiedResourceForeground": "#62b8c7",
        "list.activeSelectionForeground": "#FFD700",
        "sideBar.background": "#0f1a1e",
        "statusBar.background": "#1593a2",
        "statusBar.foreground": "#FFFFFF",
        "statusBarItem.activeBackground": "#1593a2",
        "statusBarItem.hoverBackground": "#1593a2"
    },
    "tokenColors": [
        {
            "scope": [
                "keyword.control",
                "storage.type",
                "entity.name.tag"
            ],
            "settings": { "foreground": "#FF5555", "fontStyle": "bold" }
        },
        {
            "scope": "entity.name.type.class",
            "settings": { "foreground": "#FFD700", "fontStyle": "bold" }
          },
          {
            "scope": [
                "entity.name.function",
                "support.type.property-name.css"
            ],
            "settings": { "foreground": "#50FA7B" }
          },
          {
            "scope": [
                "variable.parameter",
                "entity.name.keyword.java",
                "support.type.property-name.json",
                "storage.modifier"
            ],
            "settings": { "foreground": "#FF75C9" }
          },
          {
            "scope": "constant",
            "settings": { "foreground": "#FFA111" }
          },
          {
            "scope": "string.quoted",
            "settings": { "foreground": "#F1FA8C" }
          },
          {
            "scope": "constant.numeric",
            "settings": { "foreground": "#BD93F9" }
          },
          {
            "scope": "comment",
            "settings": { "foreground": "#6272A4", "fontStyle": "italic" }
          },
          {
            "scope": "keyword.operator",
            "settings": { "foreground": "#FFB86C" }
          },
          {
            "scope": "meta.decorator",
            "settings": { "foreground": "#00FFFF", "fontStyle": "italic" }
          },
          {
            "scope": "entity.other.attribute-name",
            "settings": { "foreground": "#FFD700" }
          },
          {
            "scope": "string.quoted.double.html, string.quoted.single.html",
            "settings": { "foreground": "#50FA7B" }
          },
          {
            "scope": "comment.block.html, comment.block.xml",
            "settings": { "foreground": "#6272A4", "fontStyle": "italic" }
          }









    ]
}