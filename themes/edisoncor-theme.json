{
    "name": "edisoncor-theme",
    "type": "dark",
    "colors": {
        "activityBarBadge.background": "#1593a2",
        "editor.background": "#1D272C",
        "editor.foreground": "#E6E6E6",
        "editor.inactiveSelectionBackground": "#3A3D41",
        "editor.lineHighlightBackground": "#3E3E3E",
        "editor.selectionBackground": "#515151",
        "editorCursor.foreground": "#FFCC00",
        "editorIndentGuide.activeBackground": "#707070",
        "editorIndentGuide.background": "#3E3E3E",
        "editorLineNumber.foreground": "#4B4B4B",
        "gitDecoration.addedResourceForeground": "#459923",
        "gitDecoration.conflictingResourceForeground": "#FFA500",
        "gitDecoration.modifiedResourceForeground": "#62b8c7",
        "list.activeSelectionForeground": "#FFD700",
        "sideBar.background": "#0f1a1e",
        "statusBar.background": "#1593a2",
        "statusBar.foreground": "#FFFFFF",
        "statusBarItem.activeBackground": "#1593a2",
        "statusBarItem.hoverBackground": "#1593a2"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#B4B4B4",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#cbc167"
            }
        },
        {
            "scope": [
                "keyword.control",
                "storage.type",
                "storage.modifier",
                "keyword.control.flow"
            ],
            "settings": {
                "foreground": "#FF75B5",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "entity.name.function.java",
                "entity.name.function.python",
                "entity.name.function.django",
                "entity.name.function.ts",
                "entity.name.function.angular",
                "function"
            ],
            "settings": {
                "foreground": "#00FFFF"
            }
        },
        {
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#FFB86C"
            }
        },
        {
            "scope": [
                "meta"
            ],
            "settings": {
                "foreground": "#62b8c7",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "constant",
            "settings": {
                "foreground": "#AC6DDB"
            }
        },
        {
            "scope": "type",
            "settings": {
                "foreground": "#8BE9FD"
            }
        },
        {
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "#FF75B5"
            }
        },
        {
            "scope": "support.type.property-name.yaml",
            "settings": {
                "foreground": "#FFB86C"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#FF75B5"
            }
        },
        {
            "scope": "string.quoted.double.json",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "scope": "punctuation.separator.key-value.json",
            "settings": {
                "foreground": "#F8F8F2"
            }
        },
        {
            "scope": "punctuation.definition.string.begin.json",
            "settings": {
                "foreground": "#F8F8F2"
            }
        },
        {
            "scope": "punctuation.definition.string.end.json",
            "settings": {
                "foreground": "#F8F8F2"
            }
        },
        {
            "scope": "constant.language.boolean",
            "settings": {
                "foreground": "#00FFFF"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#2dcc98"
            }
        },
        {
            "scope": "support.type.property-name",
            "settings": {
                "foreground": "#6ace2c"
            }
        },
        {
            "scope": "markup.heading.markdown",
            "settings": {
                "foreground": "#FF75B5",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.bold.markdown",
            "settings": {
                "foreground": "#FFD700",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.italic.markdown",
            "settings": {
                "foreground": "#FFD700",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inline.raw.markdown",
            "settings": {
                "foreground": "#8BE9FD"
            }
        }
    ]
}